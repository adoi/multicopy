{
  "name": "multicopy",
  "displayName": "multicopy",
  "description": "Copy and paste multiple snippets of code in your workspace.",
  "author": "Adonis Murati <adonismurati@gmail.com>",
  "version": "0.3.0",
  "publisher": "adoi",
  "repository": {
    "type": "git",
    "url": "https://github.com/adoi/multicopy"
  },
  "icon": "media/multicopy-icon.png",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "multicopy",
        "mac": "shift+cmd+c",
        "win": "shift+ctrl+c"
      },
      {
        "command": "multicopy.deleteItems",
        "mac": "alt+m",
        "win": "alt+m"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.43.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
